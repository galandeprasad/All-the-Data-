package Paginations;

import java.util.stream.Stream;

public class test2 {

	public static void main(String[] args)
	{
      long vales = Stream.of("prasad","Rupali","Sonali","Nikita").filter(s->{
    	  
    	  s.startsWith("a");
    	  return true;
      }).count();
      System.out.println(vales);
       
	}

}
